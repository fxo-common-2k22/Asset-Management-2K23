@model FAPP.Model.SetupModel

<div class="modal-header" style="background-color: #368ee0 !important;color:white;">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h4 class="modal-title" id="myModalLabel"> History </h4>
</div>
<div class="modal-body nopadding" style="max-height: 470px; overflow-y: auto;padding: 5px !important; overflow-x: hidden;">

    <div class="row">
        <div class="col-sm-12 downpadding">
            <table cellspacing="0" class="gv nm table table-hover no-head-border table-responsive table-striped">
                <thead class="vd_bg-green vd_white">
                    <tr>
                        <th>Sr</th>
                        <th>User</th>
                        <th>Operation</th>
                        <th>URL</th>
                        <th>Date</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model != null)
                    {
                        var count = 0;
                        foreach (var item in Model.Audits)
                        {
                            <tr class="tr_row">
                                <td><span class="label label-default current-theme">@(++count)</span></td>
                                <td>@(item.UserLog?.User?.Username)</td>
                                <td>@item.Operation</td>
                                <td>@item.Url</td>
                                <td>@item.AuditDate</td>
                            </tr>
                        }
                    }
                </tbody>


            </table>




        </div>
    </div>
</div>
<div class="modal-footer">
    <input type="button" class="btn btn-blue" data-dismiss="modal" value="Ok">
</div>

