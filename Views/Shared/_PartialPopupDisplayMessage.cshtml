@model string

<div class="modal-header modalHeader">
    <button type="button" class="close newClose" id="modal_btncross" data-dismiss="modal">&times;</button>
    <h4 class="modal-title">Message</h4>
</div>
<div class="modal-body">
    <p>
        @if (Model != null)
