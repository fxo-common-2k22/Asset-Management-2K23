@model IEnumerable<FAPP.Model.Templates1> 
<div id="table">
    <table class="table table-hover table-condensed table-striped">
        <thead>
            <tr>
                <th></th>
                <th>Title</th>
                <th>Selected Fields</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @{ int count = 1;}
            @foreach (var item in Model)
            {
                <tr>
                    <td><span class="label label-default current-theme">@(count++)</span></td>
                    <td>@Html.DisplayFor(m => item.TemplateTitle)</td>
                    <td>@Html.DisplayFor(m => item.SelectedFields)</td>
                    <td>
                        @*<a class="btn btn-success btn-sm darkblue" data-ajax="true" data-ajax-failure="OnFailure" data-ajax-method="GET" data-ajax-success="onEditSuccess" href="/Academics/CustomReportings/AddUpdateTemplate/@item.ReportTemplateId">Edit</a>*@
                        <a class="btn btn-success btn-sm darkblue" href="/CustomReporting/Add/@item.ReportTemplateId">Edit</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>

</div>

