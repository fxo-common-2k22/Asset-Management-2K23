@model FAPP.ViewModel.TemplatesViewModel
@{
    ViewBag.Title = "Templates Setting";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

 
@section TopRightMenu{
    <div id="topRightBtns">
        <ul class="minitiles">
            <li class="blue" id="editbtn">
                <a href="@FAPP.DAL.SessionHelper.ReturnUrl" data-toggle="tooltip" title="Back to Dashboard">
                    <i class="fa fa-mail-reply"></i>
                </a>
            </li>
            <li class="darkblue" data-toggle="tooltip" title="" data-original-title="Save Setings">
                <a href="javascript:" onclick="$('#saveBtn').click();">
                    <i class="fa fa-save"></i>
                </a>
            </li> 
        </ul>
    </div>
}

<div id="tempGrid">
    @Html.Partial("_PartialTemplateSetingFrm")
</div>
